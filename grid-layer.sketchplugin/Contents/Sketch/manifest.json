{
  "name": "Grid Layer",
  "description": "Create columns or gutters as layers. Useful if you need a grid on a component level",
  "version": "0.1.0",
  "author": "Jonathan Tran",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "identifier": "com.github.franklymrshankly.Grid-Layer",
  "appcast": "https://api.sketchpacks.com/v1/plugins/com.github.franklymrshankly/Grid-Layer/appcast",
  "commands": [
    {
      "name": "Create gutters",
      "identifier": "grid-layer.gutter-identifier",
      "script": "main.js",
      "handler": "gutters"
    },
    {
      "name": "Create columns",
      "identifier": "grid-layer.column-identifier",
      "script": "main.js",
      "handler": "columns"
    }
  ],
  "menu": {
    "title": "Grid layer",
    "items": [
      "grid-layer.gutter-identifier",
      "grid-layer.column-identifier"
    ]
  },
  "disableCocoaScriptPreprocessor": true
}